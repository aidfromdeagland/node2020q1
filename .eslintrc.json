{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "semi": ["error", "always"],
        "indent": ["error", 4],
        "strict": ["warn", "safe"]
    }
}
